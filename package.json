{
  "name": "vafast-benchmarks",
  "version": "1.0.0",
  "description": "综合框架性能测试 - 对比主流框架与 vafast 原生框架，集成 k6 性能测试工具",
  "main": "comprehensive-benchmark.ts",
  "type": "module",
  "scripts": {
    "dev": "bun --watch src/simple-response-benchmark.ts",
    "build": "bun build src/simple-response-benchmark.ts --target=bun --outdir=dist",
    "test:single": "k6 run k6-test-config.js",
    "start:servers": "bun run start-servers.ts",
    "test:k6:peak": "node run-k6-tests.js peak",
    "test:k6:quick": "node run-k6-tests.js quick"
  },
  "dependencies": {
    "@hono/typebox-validator": "^0.2.0",
    "@koa/router": "^14.0.0",
    "@sinclair/typebox": "^0.32.0",
    "@types/express": "^4.17.21",
    "elysia": "^1.3.13",
    "express": "^5.1.0",
    "hono": "^4.9.2",
    "koa": "^3.0.1",
    "koa-bodyparser": "^4.4.1",
    "vafast": "0.1.12"
  },
  "devDependencies": {
    "bun": "latest",
    "@types/node": "^20.10.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "bun": ">=1.0.0"
  },
  "keywords": [
    "performance",
    "benchmark",
    "framework",
    "elysia",
    "hono",
    "express",
    "vafast",
    "k6"
  ],
  "author": "Your Name",
  "license": "MIT"
}
